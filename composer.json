{
    "name": "hubeiwei/hello-yii2",
    "description": "练手用",
    "homepage": "https://github.com/hubeiwei/laohu-yii2",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "~2.0.5",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0",
        "mdmsoft/yii2-admin": "~2.0",
        "kartik-v/yii2-date-range": "dev-master",
        "kartik-v/yii2-dynagrid": "@dev",
        "kartik-v/yii2-export": "@dev",
        "kartik-v/yii2-grid": "@dev",
        "kartik-v/yii2-password": "@dev",
        "kartik-v/yii2-widget-alert": "*",
        "kartik-v/yii2-widget-datepicker": "@dev",
        "kartik-v/yii2-widget-datetimepicker": "*",
        "kartik-v/yii2-widget-fileinput": "@dev",
        "kartik-v/yii2-widget-growl": "*",
        "kartik-v/yii2-widget-select2": "@dev",
        "kartik-v/yii2-widget-sidenav": "*",
        "liyunfang/yii2-widget-linkpager": "dev-master",
        "ijackua/yii2-lepture-markdown-editor-widget": "^1.0",
        "yiidoc/yii2-redactor": "*",
        "bower-asset/highlightjs": "*"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0"
    },
    "autoload": {
        "psr-4": {
            "hubeiwei\\yii2tools\\": "hubeiwei/yii2-tools"
        }
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
